{% extends 'baseAdmin.html.twig' %}


{% block contentAdmin %}
   <div class="container" style="margin-top: 150px;">
        <div class="row">
<h1>Option</h1>
<table class="table">
	<tbody>
		<tr>
			<th>Id</th>
			<td>{{ option.id }}</td>
		</tr>
		<tr>
			<th>Name</th>
			<td>{{ option.name }}</td>
		</tr>
	</tbody>
</table>

<a class="btn btn-success mr-1" href="{{ path('app_admin_option_index') }}"><i class="fa fa-rotate-left"></i></a>

<a href="{{ path('app_admin_option_edit', {'id': option.id}) }}" class="btn btn-primary mr-1"><i class="fa fa-pen"></i></a>

{{ include('admin/option/_delete_form.html.twig') }}

        </div>
   </div>
{% endblock %}
